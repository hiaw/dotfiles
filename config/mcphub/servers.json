{
  "mcpServers": {
    "github.com/modelcontextprotocol/servers/tree/main/src/time": {
      "command": "python",
      "args": ["-m", "mcp_server_time", "--local-timezone=Pacific/Auckland"]
    },
    "github.com/mendableai/firecrawl-mcp-server": {
      "command": "npx",
      "env": {
        "FIRECRAWL_API_KEY": "${FIRECRAWL_API_KEY}"
      },
      "args": ["-y", "firecrawl-mcp"]
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/filesystem": {
      "command": "npx",
      "args": [
        "-y",
        "@modelcontextprotocol/server-filesystem",
        "${HOME}/.dotfiles"
      ]
    },
    "github.com/NightTrek/Ollama-mcp": {
      "command": "node",
      "env": {
        "OLLAMA_HOST": "http://127.0.0.1:11434"
      },
      "disabled": false,
      "args": ["${HOME}/.mcphub/servers/ollama-mcp/build/index.js"]
    },
    "github.com/modelcontextprotocol/servers/tree/main/src/git": {
      "command": "uvx",
      "args": ["mcp-server-git"]
    },
    "github.com/ahujasid/blender-mcp": {
      "command": "uvx",
      "args": ["blender-mcp"]
    }
  },
  "nativeMCPServers": {
    "neovim": {
      "disabled": false
    }
  }
}
