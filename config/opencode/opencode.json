{
  "$schema": "https://opencode.ai/config.json",
  "provider": {
    "github-copilot": {
      "models": {
        "claude-opus-4.5": {
          "variants": {
            "max": {
              "thinking": {
                "type": "enabled",
                "budgetTokens": 31999
              }
            }
          }
        }
      }
    },
    "llm-proxy": {
      "npm": "@ai-sdk/openai-compatible",
      "name": "Proxy",
      "options": {
        "baseURL": "http://127.0.0.1:8000/v1",
        "apiKey": "VerysecretKey",
        "timeout": 300000
      },
      "models": {
        "claude-opus-4-5-thinking": {
          "id": "antigravity/claude-opus-4-5",
          "name": "Claude Opus 4.5 Thinking",
          "limit": {
            "context": 200000,
            "output": 32000
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "high"
          },
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        },
        "claude-sonnet-4-5-thinking": {
          "id": "antigravity/claude-sonnet-4-5",
          "name": "Claude Sonnet 4.5 Thinking",
          "limit": {
            "context": 200000,
            "output": 32000
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "high"
          },
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        },
        "claude-sonnet-4-5": {
          "id": "antigravity/claude-sonnet-4-5",
          "name": "Claude Sonnet 4.5",
          "limit": {
            "context": 200000,
            "output": 8000
          },
          "reasoning": false,
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        },
        "ant_gemini-3-pro-high": {
          "id": "antigravity/gemini-3-pro-preview",
          "name": "ANT Gemini 3 Pro High",
          "limit": {
            "context": 1048576,
            "output": 64000
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "high"
          },
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        },
        "ant_gemini-3-pro-low": {
          "id": "antigravity/gemini-3-pro-preview",
          "name": "ANT Gemini 3 Pro Low",
          "limit": {
            "context": 1048576,
            "output": 64000
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "low"
          },
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        },
        "cli_gemini-3-pro-high": {
          "id": "gemini_cli/gemini-3-pro-preview",
          "name": "CLI Gemini 3 Pro High",
          "limit": {
            "context": 1048576,
            "output": 64000
          },
          "reasoning": true,
          "options": {
            "reasoningEffort": "high"
          },
          "modalities": {
            "input": ["image", "text"],
            "output": ["text"]
          }
        }
      }
    },
    "google": {
      "name": "Google",
      "models": {
        "antigravity-gemini-3-pro": {
          "name": "Gemini 3 Pro (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65535
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "variants": {
            "low": {
              "thinkingLevel": "low"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-gemini-3-flash": {
          "name": "Gemini 3 Flash (Antigravity)",
          "limit": {
            "context": 1048576,
            "output": 65536
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "variants": {
            "minimal": {
              "thinkingLevel": "minimal"
            },
            "low": {
              "thinkingLevel": "low"
            },
            "medium": {
              "thinkingLevel": "medium"
            },
            "high": {
              "thinkingLevel": "high"
            }
          }
        },
        "antigravity-claude-sonnet-4-5": {
          "name": "Claude Sonnet 4.5 (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          }
        },
        "antigravity-claude-sonnet-4-5-thinking": {
          "name": "Claude Sonnet 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        },
        "antigravity-claude-opus-4-5-thinking": {
          "name": "Claude Opus 4.5 Thinking (Antigravity)",
          "limit": {
            "context": 200000,
            "output": 64000
          },
          "modalities": {
            "input": ["text", "image", "pdf"],
            "output": ["text"]
          },
          "variants": {
            "low": {
              "thinkingConfig": {
                "thinkingBudget": 8192
              }
            },
            "max": {
              "thinkingConfig": {
                "thinkingBudget": 32768
              }
            }
          }
        }
      }
    }
  },
  "model": "antigravity/claude-opus-4-5",
  "small_model": "github-copilot/gpt-4.1",
  "keybinds": {
    "leader": "ctrl+l",
    "command_list": "ctrl+k",
    "variant_cycle": "alt+v",
    "messages_half_page_up": "ctrl+u",
    "messages_half_page_down": "ctrl+d"
  },
  "permission": {
    "bash": {
      "git *": "ask",
      "supabase *": "ask",
      "*": "allow"
    },
    "edit": "allow",
    "webfetch": "allow"
  },
  "agent": {
    "code-reviewer": {
      "description": "Reviews code for best practices and potential issues",
      "mode": "subagent",
      "model": "anthropic/claude-sonnet-4-20250514",
      "prompt": "You are a code reviewer. Focus on security, performance, and maintainability.",
      "tools": {
        "write": false,
        "edit": false
      }
    }
  },
  "plugin": ["oh-my-opencode@latest", "opencode-antigravity-auth@1.4.3"]
}
